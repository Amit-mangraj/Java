package com.java.programs;

public class InsertAnElementInArray {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int arr[] = new int[4];
		arr[0] = 12;
		arr[1] = 34;
		arr[2] = 54;
		arr[3] = 14;
		
		for(int i=0;i<arr.length;i++) {
			System.out.println(arr[i]);
		}
	}

}
