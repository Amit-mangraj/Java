package com.Strings.programs;

public class Basics {
	
	//String is object with sequeece of of Characters
	//A
	public static void main(String[] args) {
		String s = "try hard";
		s = "try hard";
		System.out.println(s);
		
		//string in java are immutable privious value of string remains unchanged and variable string now refer to new string
		s =s+ new String("amit");
		System.out.println(s);
	}

}
